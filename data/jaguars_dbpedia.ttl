  dbo:scientificName "Panthera onca"@en ;
  dbo:sex dbr:Male ;
  # DBpedia lacks a precise 'dietType' property, keeping local for fidelity or using 'diet'
  ont:hasDietType ont:CarnivoreDiet ;
